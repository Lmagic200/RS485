#rs485d

menu "rs485d"
source app/rs485d/Kconfig
endmenu


menu "rs485c"
source app/rs485c/Kconfig
endmenu

config ENABLE_APP_RS485D_MASTER
    bool "Enable rs485d-master"
    help
        compile rs485d-master app


config ENABLE_APP_TEST
    bool "Enable test"
    help
        compile test app
